// class Tower
public class Tower {
    int x, y;
    int range;
    int damage;
    int cooldown;
    String name;
    public Tower(int x, int y, int range, int damage, String name, int cost) {
        this.x = x;
        this.y = y;
    }

    public int getX() {
        return x;
    }

    public int getY() {
        return y;
    }

} 
