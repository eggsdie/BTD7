// class Tower
public class Tower {
    int x, y;
    int range;
    int damage;
    int speed;
    String name;
    public Tower(int x, int y, int damage, int speed) {
        this.x = x;
        this.y = y;
        this.damage = damage;
        this.speed =speed;
    }

    public int getX() {
        return x;
    }
    public int getdamage() {
        return damage;
    }
    public int speed() {
    	return speed;
    }

    public int getY() {
        return y;
    }

} 
